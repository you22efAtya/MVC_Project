@model int

<td>
    <a class="btn btn-primary" asp-action="Details" asp-route-id="@Model">
        <i class="fas fa-eye"></i> Details
    </a>
</td>
<td>
    <a class="btn btn-success" asp-action="Edit" asp-route-id="@Model">
        <i class="fas fa-edit"></i> Edit
    </a>
</td>
<td>
    <button type="button" class="btn btn-danger" data-bs-toggle="modal" data-bs-target="#deleteModal-@Model">
        <i class="fas fa-trash"></i> Delete
    </button>
</td>